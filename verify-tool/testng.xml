<?xml version="1.0" encoding="UTF-8"?>

<!-- Please make sure to remove TestMethodListener in VerifyTests class before running this xml-->

<suite thread-count="4" name="testng-verify-tool" parallel="methods">

	<listeners>
		<listener class-name="org.muthu.TestMethodListener" />
	</listeners>

	<test name="VerifyTests" preserve-order="false">
		<classes>
			<class name="org.muthu.VerifyTests" />
		</classes>
	</test>
</suite>